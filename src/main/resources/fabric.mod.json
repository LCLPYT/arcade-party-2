{
  "schemaVersion": 1,
  "id": "ap2",
  "version": "${version}",
  "name": "Arcade Party 2",
  "description": "An arcade minigame for modern Minecraft where the players must win as many challenges as possible in order to win the game!",
  "authors": [
    "LCLP",
    "bops"
  ],
  "contact": {
    "homepage": "https://github.com/LCLPYT/arcade-party-2",
    "sources": "https://github.com/LCLPYT/arcade-party-2"
  },
  "license": "MIT",
  "environment": "server",
  "entrypoints": {
    "game-entry": [
      "work.lclpnet.ap2.base.ArcadeParty"
    ]
  },
  "mixins": [
    "ap2.mixins.json",
    "ap2-debug.mixins.json"
  ],
  "accessWidener": "ap2.accesswidener",
  "depends": {
    "fabricloader": ">=0.15.11",
    "minecraft": "~1.21",
    "java": ">=21",
    "mg-lobby": "*",
    "kibu": "*",
    "notica": "*",
    "combat-control": "*",
    "fantasy": "*",
    "kibu-world-api": "*",
    "sqlite-jdbc": "*"
  },
  "suggests": {
    "pal": "*",
    "mcserver-api": "*"
  }
}